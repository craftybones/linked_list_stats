{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "A simple bar chart with embedded data.",
  "data": {
    "name": "foo",
    "url": "/final_data.json"
  },
  "title": "Map Filter Reduce Pass Percentages",
  "config": {
    "axis": {
      "labelFontSize": 12
    }
  },
  "mark": "bar",
  "width": 800,
  "encoding": {
    "x": { "field": "percent", "type": "quantitative" },
    "y": {
      "field": "name",
      "type": "nominal",
      "sort": { "field": "percent", "order": "descending" }
    },
    "color": {
      "field": "percent",
      "type": "quantitative",
      "scale": {
        "scheme": "lightmulti",
        "reverse": true
      }
    }
  }
}
